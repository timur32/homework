server {
   server_name myvm.localhost {{ ansible_default_ipv4.address }};
   listen 80 default_server;

   location / {
     root /var/www/html;
     rewrite ^/(.*) /$1 break;
     proxy_pass http://127.0.0.1:5001;
     proxy_set_header Host $host;
     proxy_set_header X-Real-IP ip_address;
  }
}

